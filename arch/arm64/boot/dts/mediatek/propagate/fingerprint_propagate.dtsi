&spi0 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";
	gf_fingerprint@0 {
		compatible = "goodix,goodix-fp";
		reg = <0>;
		spi-max-frequency = <8000000>;
		status = "okay";
	};

	silead_fp@1 {
		compatible = "sil,silead_fp";
		reg = <1>;
		spi-max-frequency = <8000000>;
		status = "okay";
	};
};

&pio {
	fingerprint_irq: fingerprint_irq {
		pins_cmd_dat {
			pins = <PINMUX_GPIO9__FUNC_GPIO9>;
			slew-rate = <0>;
			bias-pull-down = <00>;
		};
	};

	fingerprint_power_high: state_power_high {
		pins_cmd_dat {
			pins = <PINMUX_GPIO26__FUNC_GPIO26>;
			slew-rate = <1>;
			output-high;
			bias-disable;
		};
	};

	fingerprint_power_low: state_power_low {
		pins_cmd_dat {
			pins = <PINMUX_GPIO26__FUNC_GPIO26>;
			slew-rate = <1>;
			output-low;
			bias-disable;
		};
	};

	fingerprint_reset_high: state_reset_high {
		pins_cmd_dat {
			pins = <PINMUX_GPIO13__FUNC_GPIO13>;
			slew-rate = <1>;
			output-high;
			bias-disable;
		};
	};

	fingerprint_reset_low: state_reset_low {
		pins_cmd_dat {
			pins = <PINMUX_GPIO13__FUNC_GPIO13>;
			slew-rate = <1>;
			output-low;
			bias-disable;
		};
	};
};

&goodix_fp {
	interrupt-parent = <&pio>;
	interrupts = <9 IRQ_TYPE_EDGE_RISING 9 0>;

	pinctrl-names = "fingerprint_irq", "power_high", "power_low", "reset_high", "reset_low";
	pinctrl-0 = <&fingerprint_irq>;
	pinctrl-1 = <&fingerprint_power_high>;
	pinctrl-2 = <&fingerprint_power_low>;
	pinctrl-3 = <&fingerprint_reset_high>;
	pinctrl-4 = <&fingerprint_reset_low>;
	status = "okay";
};

&silead_fp {
	interrupt-parent = <&pio>;
	interrupts = <9 IRQ_TYPE_EDGE_RISING 9 0>;

	pinctrl-names = "irq-init", "avdd-enable", "power_low", "rst-high", "rst-low";
	pinctrl-0 = <&fingerprint_irq>;
	pinctrl-1 = <&fingerprint_power_high>;
	pinctrl-2 = <&fingerprint_power_low>;
	pinctrl-3 = <&fingerprint_reset_high>;
	pinctrl-4 = <&fingerprint_reset_low>;
	status = "okay";
};

